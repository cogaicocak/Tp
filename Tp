local SPAM_TP_COUNT = 50 -- S·ªë l·∫ßn spam TP
local SPAM_TP_INTERVAL = 0.01 -- Delay gi·ªØa m·ªói l·∫ßn TP (c√†ng nh·ªè c√†ng t·ªët)
local MAX_DIRECT_TP_DISTANCE = 8000

